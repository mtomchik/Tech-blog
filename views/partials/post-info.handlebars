<article class="entry">
    <div class="whole-entry">
        <div class="info-section">
            <div class="entry-name">
                <a href="/entry/{{id}}" class="links">{{title}}</a>
            </div>
            <div class="created-by">
                <p id="comment-subtitle">
                    Entryed by <b>{{user.username}} </b> on <b>{{formatDate created_at}}</b>
                </p>
            </div>
        </div>
        <div class="content-section">
            <p>{{content}}</p>
        </div>
    </div>
</article>